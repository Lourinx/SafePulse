<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.DoctorProfileController"
            prefWidth="760" prefHeight="600">

    <Label layoutX="30" layoutY="40" text="Doctor Profile" style="-fx-font-size: 24px; -fx-font-weight: bold;" />

    <Label layoutX="30" layoutY="100" text="Full Name:" />
    <Label fx:id="nameLabel" layoutX="150" layoutY="100" />

    <Label layoutX="30" layoutY="140" text="Username:" />
    <Label fx:id="usernameLabel" layoutX="150" layoutY="140" />

    <Label layoutX="30" layoutY="180" text="License Number:" />
    <Label fx:id="licenseLabel" layoutX="150" layoutY="180" />

    <Label layoutX="30" layoutY="220" text="Registered Since:" />
    <Label fx:id="registrationLabel" layoutX="150" layoutY="220" />
</AnchorPane>

